@model IEnumerable<SymphonyLimited.DbContext.Exam_Tbl>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Exam</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Exam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Exam_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Faculty_Tbl.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student_Tbl.Student_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Exam)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Exam_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Faculty_Tbl.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Student_Tbl.Student_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
