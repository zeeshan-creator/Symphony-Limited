@model  SymphonyLimited.DbContext.Student_Tbl

@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<hr />
<h2 class="my-3">Login</h2>

@using (Html.BeginForm())
{
    <div class="mx-5" >
        <div class="form-group">
            <label for="Email">Email</label>
            @Html.EditorFor(model => model.Email2, new { htmlAttributes = new { @class = "form-control", @Required = "", @placeholder = "Enter Email", id = "Email" } })
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Password</label>
            @Html.EditorFor(model => model.Password2, new { htmlAttributes = new { @class = "form-control", @Required = "", @placeholder = "Enter Password", id = "Email" } })

        </div>
        <button type="submit" class="btn btn-primary">Login</button>
    </div>
}